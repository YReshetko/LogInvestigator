<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.my.home.ui.controllers.MainWindowController">
   <left>
      <VBox maxWidth="300.0" minWidth="20.0" prefHeight="2000.0" BorderPane.alignment="CENTER">
         <children>
            <TreeView fx:id="logTree" focusTraversable="false" prefHeight="2000.0" />
         </children>
      </VBox>
   </left>
   <center>
      <Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="300.0" BorderPane.alignment="CENTER">
         <children>
            <TabPane minHeight="200.0" prefHeight="2000.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Selectors">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="2000.0" prefWidth="200.0" />
                  </content>
                </Tab>
                <Tab text="Filters">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="2000.0" prefWidth="200.0" />
                  </content>
                </Tab>
                  <Tab text="Processors">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="2000.0" prefWidth="200.0" />
                     </content>
                  </Tab>
                  <Tab text="PostProcessors">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="2000.0" prefWidth="300.0" />
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
            <Accordion prefHeight="2000.0">
              <panes>
                <TitledPane animated="false" text="Selector">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="450.0" />
                  </content>
                </TitledPane>
                <TitledPane animated="false" text="Filters">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="450.0" />
                  </content>
                </TitledPane>
                <TitledPane animated="false" text="Processors">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="450.0" />
                  </content>
                </TitledPane>
                  <TitledPane animated="false" layoutX="10.0" layoutY="60.0" text="PostProcessors">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="450.0" />
                     </content>
                  </TitledPane>
              </panes>
            </Accordion>
         </children>
      </VBox>
   </right>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Save as ..." textAlignment="CENTER">
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
            </Button>
         </children>
      </HBox>
   </bottom>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleOptionButton" text="Options" />
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
